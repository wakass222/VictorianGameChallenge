            <string>///Calculate positions

fileName = global.storySelected[? "text"+global.ending]
fileID = file_text_open_read(global.endingDirectory + fileName);
text = file_text_read_string(fileID);
file_text_close(fileID);

image = global.storySelected[? "sprite"+global.ending];

draw_set_font(global.font);
totalHeight = sprite_get_height(image) + string_height_ext(text, global.newLineSpacing, view_wview[0] - global.padding * 4) + (global.padding * 6) + view_hview[0] * 0.1;
startHeight = (view_hview[0] - totalHeight)/2;

drawImageHeight = startHeight;
drawTextHeight = drawImageHeight + sprite_get_height(image) + (global.padding * 4);

drawContinueButtonHeight = drawTextHeight + string_height_ext(text, global.newLineSpacing, view_wview[0] - global.padding * 4) + global.padding * 2 + (view_hview[0] * 0.1)/2;
instance_create(view_wview[0]/2, drawContinueButtonHeight, obj_continue);


</string>
